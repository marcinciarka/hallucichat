{
  "name": "hallucichat",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run dev:next\" \"npm run dev:server\"",
    "dev:next": "next dev",
    "dev:server": "ts-node --project server/tsconfig.json server/index.ts",
